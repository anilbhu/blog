<h1>my blog</h1>
<hr></hr>
<%@post.each do |post| %>
<h1><%=post.title %></h1>
<p><%=post.content %></p>
<p><%=link_to 'show',post %></p>
<p><%=link_to 'edit', edit_post_path(post) %>
<p><%=link_to 'Delete', delete_post_path(:id => post.id) %></p>
</hr>
<% end %>
<p><%=link_to "add a new post",new_post_path %></p>
