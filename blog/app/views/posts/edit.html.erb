<h1>editpost</h1>
<%=form_for @post do |f| %>
<p>
<br><%=f.label :title %></br>
<br><%=f.text_field :title %></br>
</p>
<p>
<br><%=f.label :content %></br>
<br><%=f.text_field :content %></br>
</p>
<p>
<h1><%=f.submit "updatepost" %></h1>
</p>
<% end %>
<h1><%=link_to 'back',posts_path(@post) %></h1>
